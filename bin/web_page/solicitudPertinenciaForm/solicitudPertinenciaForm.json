    "listaDocentes" : {
      "type" : "url",
      "value" : [
        "../API/bpm/caseVariable/{{task.caseId}}/listaDocentes"
      ],
      "advancedOptions" : {
        "headers" : "",
        "statusCode" : "",
        "failedResponseValue" : ""
      },
      "exposed" : false
    },
    "obtenerLista" : {
      "type" : "expression",
      "value" : [
        "let va = $data.listaDocentes.value\r",
        "\r",
        "\r",
        "// Suponiendo que tienes la cadena de texto en la variable 'response'\r",
        "var valueString = va;\r",
        "\r",
        "// Extraer los objetos UserImpl de la cadena de texto utilizando expresiones regulares\r",
        "var regex = /UserImpl\\{.*?\\}/g;\r",
        "var matches = valueString.match(regex);\r",
        "\r",
        "// Crear una lista de objetos JavaScript a partir de los matches\r",
        "var listaDocentes = [];\r",
        "matches.forEach(function(match) {\r",
        "  var docente = {};\r",
        "\r",
        "  // Extraer los atributos del objeto UserImpl\r",
        "  var id = match.match(/id=(\\d+)/)[1];\r",
        "  var firstName = match.match(/firstName='(.*?)'/)[1];\r",
        "  var lastName = match.match(/lastName='(.*?)'/)[1];\r",
        "  var userName = match.match(/userName='(.*?)'/)[1];\r",
        "\r",
        "  // Asignar los atributos al objeto docente\r",
        "  docente.id = id;\r",
        "  docente.fullName = firstName + \" \" + lastName;\r",
        "  docente.userName = userName;\r",
        "\r",
        "  // Agregar el objeto docente a la lista\r",
        "  listaDocentes.push(docente);\r",
        "});\r",
        "\r",
        "console.log(listaDocentes);\r",
        "\r",
        "return listaDocentes;"
      ],
      "exposed" : false
    },
