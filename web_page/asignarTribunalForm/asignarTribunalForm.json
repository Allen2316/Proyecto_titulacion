    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.sustentacion && $data.sustentacion_tribunal ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.sustentacion.tribunal = $data.sustentacion_tribunal;",
        "\treturn {",
        "\t\tpresidente: $data.formInput.presidente,",
        "\t\tvocal_p: $data.formInput.vocal_p,",
        "\t\tvocal_s: $data.formInput.vocal_s,",
        "\t\t//map sustentacion variable to expected task contract input",
        "\t\tsustentacionInput: {",
        "\t\t\tlugar: $data.sustentacion.lugar !== undefined ? $data.sustentacion.lugar : null,",
        "\t\t\tfecha: $data.sustentacion.fecha !== undefined ? $data.sustentacion.fecha : null,",
        "\t\t\thora: $data.sustentacion.hora !== undefined ? $data.sustentacion.hora : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
