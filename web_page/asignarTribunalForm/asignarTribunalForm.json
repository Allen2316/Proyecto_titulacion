    "obtenerLista" : {
      "type" : "expression",
      "value" : [
        "let va = $data.listaDocentes.value\r",
        "\r",
        "\r",
        "// Suponiendo que tienes la cadena de texto en la variable 'response'\r",
        "var valueString = va;\r",
