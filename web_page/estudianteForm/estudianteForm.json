    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.adjuntarTIC ){",
        "\treturn {",
        "\t    adjuntarTICInput: $data.formInput.adjuntarTICInput,",
        "\t\tdocTICDocumentInput: $data.formInput.docTICDocumentInput,",
        "\t\tdocSolicitudTICDocumentInput: $data.formInput.docSolicitudTICDocumentInput",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
