    "isValidFileWidgetValue" : {
      "type" : "expression",
      "value" : [
        "// Esta expresión devuelve true si se cumplen alguna de las siguientes condiciones:\r",
        "// - Si no se ha configurado ningún widget de archivo (objeto JSON vacío: {}).\r",
        "// Lo hacemos así para no mostrar un mensaje de advertencia si el usuario no seleccionó ningún archivo.\r",
        "// - Si el archivo seleccionado y subido (la carga se activa inmediatamente después de la selección) es un archivo PDF\r",
        "\r",
        "var atLeastOnePdf = false;\r",
        "if (angular.isDefined($data.formOutput)) {\r",
        "    if (angular.isArray($data.formOutput.docAptitudLegalDocumentInput)) {\r",
        "        atLeastOnePdf = $data.formOutput.docAptitudLegalDocumentInput.some(function(document) {\r",
        "            return document.contentType == \"application/pdf\";\r",
        "        });\r",
        "    }\r",
        "}\r",
        "\r",
        "return (angular.equals(null, $data.formOutput.docTICDocumentInput) || $data.formOutput.docTICDocumentInput.contentType == \"application/pdf\")\r",
        "    && atLeastOnePdf;"
      ],
      "exposed" : false
    }
  },
