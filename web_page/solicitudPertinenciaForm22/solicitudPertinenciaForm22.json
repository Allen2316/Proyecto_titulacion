    "obtenerLista" : {
      "type" : "expression",
      "value" : [
        "let va = $data.listaDocentes.value",
        "",
        "",
        "// Suponiendo que tienes la cadena de texto en la variable 'response'",
        "var valueString = va;",
        "",
        "// Extraer los objetos UserImpl de la cadena de texto utilizando expresiones regulares",
        "var regex = /UserImpl\\{.*?\\}/g;",
        "var matches = valueString.match(regex);",
        "",
        "// Crear una lista de objetos JavaScript a partir de los matches",
        "var listaDocentes = [];",
        "matches.forEach(function(match) {",
        "  var docente = {};",
        "",
        "  // Extraer los atributos del objeto UserImpl",
        "  var id = match.match(/id=(\\d+)/)[1];",
        "  var firstName = match.match(/firstName='(.*?)'/)[1];",
        "  var lastName = match.match(/lastName='(.*?)'/)[1];",
        "  var email = match.match(/email='(.*?)'/)[1];",
        "",
        "  // Asignar los atributos al objeto docente",
        "  docente.id = id;",
        "  docente.email = email;",
        "  docente.fullName = firstName + \" \" + lastName + \" - \" + email;",
        "  ",
        "",
        "  // Agregar el objeto docente a la lista",
        "  listaDocentes.push(docente);",
        "});",
        "",
        "listaDocentes.foreach( d =>{",
        "    console.log(`Id ${d.id} Email ${d.email} Nombres ${d.fullName}`)",
        "})",
        "",
        "",
        "return listaDocentes;"
      ],
      "exposed" : false
    },
    "listaDocentes" : {
      "type" : "url",
      "value" : [
        "../API/bpm/caseVariable/{{task.caseId}}/listaDocentes"
      ],
